{
  "name": "@whimmy-ai/whimmy",
  "version": "0.1.0",
  "description": "Whimmy channel plugin for OpenClaw",
  "main": "index.ts",
  "type": "module",
  "files": [
    "index.ts",
    "src",
    "openclaw.plugin.json"
  ],
  "scripts": {
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "openclaw",
    "whimmy",
    "channel"
  ],
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "chalk": "^5.6.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.2.13"
  },
  "openclaw": {
    "id": "whimmy",
    "extensions": [
      "./index.ts"
    ],
    "channels": [
      "whimmy"
    ],
    "installDependencies": true,
    "channel": {
      "id": "whimmy",
      "label": "Whimmy",
      "selectionLabel": "Whimmy",
      "docsPath": "/channels/whimmy",
      "blurb": "Whimmy multi-agent messenger channel via WebSocket.",
      "order": 80,
      "aliases": []
    },
    "install": {
      "npmSpec": "@whimmy-ai/whimmy",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  }
}
